@model IEnumerable<Corporation>
@{
    ViewData["Title"] = "Corporation";
}
<style>
    h1{
        font-family:'Hakgyoansim Wooju';
        font-display:auto;
        font-synthesis-style:auto;
    }
    table{
        font-family:'Hakgyoansim Wooju';
        font-size:large;
        text-align:center;
    }
</style>
<h1 class="fw-bolder text-primary text-center">기업 조회</h1>
<form asp-controller="Home" asp-action="Corporation">
    <p>
            <span>
                <button type="submit" class="btn btn-secondary float-end">
                    <span>검색</span>
                </button>
            </span>
            <input class="form-control-sm float-end" type="text" list="Coparation" name="searchString" placeholder="회사를 검색하세요."/>
            <datalist id="Coparation">
                <option value="LG 에너지 솔루션"></option>
                <option value="삼성전자"></option>
                <option value="현대자동차"></option>
                <option value="우아한형제들"></option>
                <option value="SK 하이닉스"></option>
                <option value="카카오"></option>
                <option value="한국전력공사"></option>
                <option value="기아"></option>
                <option value="쿠팡"></option>
                <option value="농심"></option>
                <option value="빙그레"></option>
                <option value="아모레퍼시픽"></option>
                <option value="토스"></option>
            </datalist>
    </p>
</form>
<br />
<br />
<div class="table-responsive-lg">
    <table class="table table-sm table-bordered border-dark">
    <thead class="table-light">
        <tr>
            <th class="align-middle">
                번호
            </th>
            <th class="align-middle">
                기업
            </th>
            <th class="align-middle">
                계열
            </th class="align-middle">
            <th><a asp-action="ResumeWrite" class="btn btn-info">이력서 쓰기</a></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="table-light">
                <td class="align-middle">
                    @item.No
                </td>
                <td class="align-middle">
                    @{
                            if(item.No == 1)
                            {
                                <img width="40" height="40" class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 2)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 3)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 4)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 5)
                            {
                                <img width="40" height="40" class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 6)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 7)
                            {
                                <img width="50" height="50" class="img-thumbnail" src="https://i.namu.wiki/i/hjp9uqjiydMxKMKK8wmVJNtK5wdpM89xai28rCS7264PMEiA4aU1g3cPl32MvBXKcZ-OOTs8ayTaLTAC5OXernUAv_-Tmkdo9T4xM-NHT0kpeD98CXOEEyRwtd3BT4o6_lKBylbIVApLeKOKDSnrPA.svg"/>
                            }
                            else if(item.No == 8)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 9)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 10)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 11)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 12)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                            else if(item.No == 13)
                            {
                                <img class="img-thumbnail" src="data:image/png;base64,"/>
                            }
                        }
                    @item.CorporationName
                </td>
                <td class="align-middle">
                    @item.Line
                </td class="align-middle">
                <td>
                    @{
                        if (item.No == 1)
                        {
                                <a href="https://www.lgensol.com/kr/index" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://www.lgensol.com/kr/career-recruit" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 2)
                        {
                                <a href="https://www.samsung.com/sec/" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://www.samsungcareers.com/hr/" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 3)
                        {
                                <a href="https://www.hyundai.com/kr/ko/e" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://www.hyundai.co.kr/recruit/recruitMain" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 4)
                        {
                                <a href="https://www.woowahan.com/" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://career.woowahan.com/?utm_source=google&utm_medium=sa&utm_campaign=career&utm_content=baemin&gclid=Cj0KCQjwgNanBhDUARIsAAeIcAt5kzeLHfRYJwo6WGiVVf_Fb_cThZy_koLJn7D-su8T6h4OMFScAp8aAkJ6EALw_wcB" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 5)
                        {
                                <a href="https://www.skhynix.com/" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://recruit.skhynix.com/servlet/mnus_main.view" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 6)
                        {
                                <a href="https://www.kakaocorp.com/page/" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://careers.kakao.com/index" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 7)
                        {
                                <a href="https://home.kepco.co.kr/kepco/main.do" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://recruit.kepco.co.kr:444/frt/main.do" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 8)
                        {
                                <a href="https://www.kia.com/kr" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://career.kia.com/main/main.kc" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 9)
                        {
                                <a href="https://www.coupang.com/" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://www.coupang.jobs/kr/" target="_blank" class="btn btn-secondary">채용정보</a>
                        }
                               
                        else if (item.No == 10)
                        {
                                <a href="https://www.nongshim.com/main/index" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://recruit.nongshim.com/recruit/notice/list" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 11)
                        {
                                <a href="https://www.bing.co.kr/" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://recruit.bing.co.kr/app/web/main/index_new.jsp" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                       else if (item.No == 12)
                        {
                                <a href="https://www.apgroup.com/int/ko/" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://careers.apgroup.com/" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                        else if (item.No == 13)
                        {
                                <a href="https://toss.im/" target="_blank" class="btn btn-secondary">홈페이지</a>
                                <a href="https://toss.im/career" target="_blank" class="btn btn-secondary">채용정보</a>
                                
                        }
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
</div>